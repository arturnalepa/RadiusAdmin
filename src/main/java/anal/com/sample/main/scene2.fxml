<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="432.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="anal.com.sample.main.scene2Controler">
    <children>
        <Pane layoutX="-1.0" prefHeight="432.0" prefWidth="600.0">
         <children>
            <TableView fx:id="personTable" editable="true" layoutX="7.0" layoutY="133.0" onMouseClicked="#RightContextMenu" prefHeight="296.0" prefWidth="590.0" tableMenuButtonVisible="true">
              <columns>
                <TableColumn fx:id="colmacaddress" prefWidth="75.0" text="C1" />
                <TableColumn fx:id="colmacpassword" prefWidth="75.0" text="C2" />
                 <TableColumn fx:id="colvlan" prefWidth="75.0" text="C2" />

              </columns>
               <contextMenu>
                  <ContextMenu>
                    <items>
                      <MenuItem fx:id="Edit" mnemonicParsing="false" onAction="#Edytuj" text="Edytuj" />
                        <MenuItem mnemonicParsing="false" text="Unspecified Action" />
                    </items>
                  </ContextMenu>
               </contextMenu>
            </TableView>
            <TextField fx:id="MacAddress" layoutX="14.0" layoutY="61.0" onKeyPressed="#insertValue" promptText="MacAddress" />
            <TextField fx:id="MacPassword" layoutX="14.0" layoutY="100.0" promptText="MacPassword" />
            <ComboBox fx:id="VlanGroup" layoutX="198.0" layoutY="61.0" onAction="#getSelectedVlan" prefWidth="150.0" promptText="Vlan" />
            <CheckBox fx:id="checkMacPass" layoutX="24.0" layoutY="28.0" mnemonicParsing="false" onAction="#status" text="MacAddress=Password" />
            <Label fx:id="LMaCAddress" layoutX="163.0" layoutY="65.0" text="" />
            <Button fx:id="AddClient" layoutX="198.0" layoutY="100.0" mnemonicParsing="false" onAction="#DodajHosta" prefHeight="25.0" prefWidth="150.0" text="Add" />
         </children>
        </Pane>
    </children>
</AnchorPane>
